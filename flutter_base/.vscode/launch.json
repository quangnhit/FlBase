    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_base dev",
            "request": "launch",
            "program": "lib/main_dev.dart",
            "type": "dart",
            "args": [
                "--flavor",
                "uat"
            ]
        },
        {
            "name": "flutter_base prod",
            "request": "launch",
            "program": "lib/main_prod.dart",
            "type": "dart",
            "args": [
                "--flavor",
                "prod"
            ]
        },
        {
            "name": "flutter_base (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_base (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
